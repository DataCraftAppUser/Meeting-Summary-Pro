# Environment Variables - Frontend
# Copy this to .env and fill in your actual values

# Supabase (Frontend uses anon key, NOT service_role!)
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Backend API
REACT_APP_API_URL=http://localhost:5000/api
# In production: https://your-backend.vercel.app/api

# App Config
REACT_APP_NAME="Meeting Summary Pro"
REACT_APP_VERSION="1.0.0"

# Features (optional toggles)
REACT_APP_ENABLE_TIME_TRACKING=false
REACT_APP_AUTO_SAVE_INTERVAL=60000
